<resources>

	<style name="HorizontalDivider">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">1px</item>
		<item name="android:background">#FFE6E6E6</item>
	</style>

	<style name="VerticalDivider">
		<item name="android:layout_width">1px</item>
		<item name="android:layout_height">0dp</item>
		<item name="android:background">#FFE6E6E6</item>
	</style>

	<style name="Shadow">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">7dp</item>
		<item name="android:background">@drawable/sh_shadow_top</item>
	</style>

	<style name="ShadowBottom">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">7dp</item>
		<item name="android:background">@drawable/sh_shadow_bottom</item>
	</style>

</resources>
