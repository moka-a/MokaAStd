<resources>

	<style name="HorizontalDivider">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">1px</item>
		<item name="android:background">#FFE6E6E6</item>
	</style>

	<style name="VerticalDivider">
		<item name="android:layout_width">1px</item>
		<item name="android:layout_height">0dp</item>
		<item name="android:background">#FFE6E6E6</item>
	</style>

	<style name="Shadow">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">7dp</item>
		<item name="android:background">@drawable/sh_shadow_top</item>
	</style>

	<style name="ShadowBottom">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">7dp</item>
		<item name="android:background">@drawable/sh_shadow_bottom</item>
	</style>

	<style name="dialogButtonTextStyle">
		<item name="android:textColor">@color/black_04</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:minWidth">62dp</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
		<item name="android:layout_marginRight">8dp</item>
		<item name="android:textSize">12dp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:background">?attr/selectableItemBackground</item>
		<item name="android:gravity">center</item>
	</style>

</resources>
